@using NALSTEST.Models.UserModel
@model NALSTEST.Models.UserModel.UserProfileViewModel
@{
    ViewBag.Title = "Home Page";
    var userLogin = ViewBag.UserLogin as UserLoginViewModel;
}
@{
    Html.RenderPartial("_NavigationTitleLink", "");
}
<div class="page-content">
    <div class="page-content-area">
        <div class="page-header">
            <h1>
               <span>Thông tin đăng nhập</span> <small><i class="default-icon fa fa-angle-double-right"></i> <span>HTEST_NALS </span></small>
            </h1>
        </div>
        @if (!string.IsNullOrEmpty(ViewBag.Notify))
        {
            <h4 class="red" style="font-size: 16px">@ViewBag.Notify</h4>
            <div class="space-8"></div>
        }
        @if (!string.IsNullOrEmpty(ViewBag.ProductNotify))
        {
             <h4 class="red" style="font-size: 16px">@ViewBag.ProductNotify</h4>
            <div class="space-8"></div>
        }
        <div class="row">
            <div class="col-xs-12">
                <form class="form-horizontal" role="form">
                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right">Tên đăng nhập:</label>
                        <div class="col-sm-9">
                            <strong><span>@userLogin.UserName</span></strong>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right">Họ và tên:</label>
                        <div class="col-sm-9">
                            <strong><span>@userLogin.FullName</span></strong>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right">Chi nhánh:</label>
                        <div class="col-sm-9">
                            <strong><span>@Model.Branch</span></strong>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right">Phòng giao dịch:</label>
                        <div class="col-sm-9">
                            <strong><span>@userLogin.PosCode</span></strong>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right">Lần đăng nhập cuối:</label>
                        <div class="col-sm-9">
                            <strong><span>@Model.LastLogin</span></strong>
                        </div>
                    </div>
                    @*<div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right">Ip đăng nhập cuối:</label>
                        <div class="col-sm-9">
                            <strong><span>@Model.LastIpLogin</span></strong>
                        </div>
                    </div>*@
                </form>
            </div>
        </div>
    </div>
</div>
